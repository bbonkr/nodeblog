{
    "name": "nodeblog",
    "version": "0.1.0",
    "description": "document sharing service prototype app. node backend + react.js frontend",
    "author": "Pon Cheol Ku",
    "license": "MIT",
    "homepage": "https://github.com/bbonkr/nodeblog",
    "repository": {
        "type": "git",
        "url": "https://github.com/bbonkr/nodeblog.git"
    },
    "bugs": {
        "url": "https: //github.com/bbonkr/nodeblog/issues"
    },
    "engines": {
        "npm": ">=6.7.0",
        "node": ">=11.13.0"
    },
    "main": "index.js",
    "scripts": {
        "test": "react-scripts test --testMatch ./**/*.test.{js,jsx,ts,tsx}",
        "dev": "cross-env PORT=5000 HOST=localhost PROTOCOl=http nodemon",
        "sequelize": "node_modules/.bin/sequelize",
        "build": "cross-env BUNDLE_ANALYZER=both next build",
        "prestart": "npm run build",
        "start": "cross-env NODE_ENV=production node index.js",
        "next:start": "cross-env NODE_ENV=production next start -p 5000"
    },
    "dependencies": {
        "@testing-library/react": "^8.0.1",
        "@types/jest": "^24.0.13",
        "@types/next": "^8.0.5",
        "@types/react": "^16.8.19",
        "@types/react-dom": "^16.8.4",
        "@types/react-redux": "^7.0.9",
        "@types/sequelize": "^4.28.3",
        "@zeit/next-bundle-analyzer": "^0.1.2",
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-sass": "^1.0.1",
        "@zeit/next-typescript": "^1.1.1",
        "acorn": "^6.1.1",
        "antd": "^3.18.2",
        "axios": "^0.19.0",
        "babel-eslint": "^10.0.1",
        "bcrypt": "^3.0.6",
        "compression-webpack-plugin": "^2.0.0",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "cross-env": "^5.2.0",
        "dotenv": "^8.0.0",
        "draft-js": "^0.10.5",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.13.0",
        "express": "^4.17.0",
        "express-session": "^1.16.1",
        "helmet": "^3.18.0",
        "hpp": "^0.2.2",
        "immer": "^3.1.2",
        "jest": "^24.8.0",
        "jest-dom": "^3.4.0",
        "mariadb": "^2.0.5",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.1",
        "mysql2": "^1.6.5",
        "next": "^8.1.0",
        "next-redux-saga": "^4.0.2",
        "next-redux-wrapper": "^3.0.0-alpha.2",
        "node-sass": "^4.12.0",
        "nodemon": "^1.19.0",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-draft-wysiwyg": "^1.13.2",
        "react-helmet": "^5.2.1",
        "react-markdown": "^4.0.8",
        "react-redux": "^7.1.0-alpha.5",
        "react-scripts": "^3.0.1",
        "react-slick": "^0.24.0",
        "redux": "^4.0.1",
        "redux-saga": "^1.0.2",
        "sequelize": "^5.8.6",
        "sequelize-cli": "^5.4.0",
        "showdown": "^1.9.0",
        "showdown-xss-filter": "^0.2.0",
        "styled-components": "^4.2.0",
        "webpack": "^4.32.1",
        "webpack-ant-icon-loader": "^1.0.5",
        "webpack-bundle-analyzer": "^3.3.2"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^8.1.0",
        "eslint-plugin-react-hooks": "^1.6.0",
        "typescript": "^3.5.2"
    },
    "eslintConfig": {
        "extends": "react-app",
        "plugins": [
            "react-hooks"
        ],
        "rules": {
            "react-hooks/rules-of-hooks": "error",
            "react-hooks/exhaustive-deps": "warn"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "**/*.{js,jsx,ts,tsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/**/__tests__/**/*.*",
            "!<rootDir>/**/*.test.*"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    }
}
